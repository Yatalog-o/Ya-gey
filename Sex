Tabs.Main:CreateButton{
    Title = "Продать всех брейнротов в рабство",
    Description = "Я гидросексуал",
    Callback = function()
game:GetService("ReplicatedStorage").Remotes.ItemSell:FireServer() 
end}
local Slider = Tabs.Main:CreateSlider("Slider", {
    Title = "Slider",
    Description = "Перетягивай",
    Default = 2,
    Min = 0,
    Max = 1000,
    Rounding = 1,
    Callback = function(Value)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
    end
})

Slider:OnChanged(function(Value)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
end)

Slider:SetValue(30)
